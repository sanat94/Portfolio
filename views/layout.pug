    // Contact Section
    section#contact.parallax-section
      .container
        .row
          .contact-form
            .wow.fadeInUp.section-title(data-wow-delay='0.2s')
              h1.color-white Say hello..
              p.color-white Fill out your details for me to contact you. Thank You!
            #contact-form
              form(action='https://mail.google.com/mail/u/0/', method='post')
                .wow.fadeInUp(data-wow-delay='1s')
                  #form-group 
                    input.form-control(name='name', type='text', placeholder='Your Name')
                .wow.fadeInUp(data-wow-delay='1.1s')
                  #form-group 
                    input.form-control(name='email', type='text', placeholder='Your Email',  required='')
                .wow.fadeInUp(data-wow-delay='1.2s')
                  #form-group
                    textarea.form-control(name='message', rows='5', placeholder='Write your message...')
                .wow.fadeInUp.col-md-6.col-sm-8(data-wow-delay='1.3s')
                   #form-group
                    input.form-control(name='submit', type='submit', value='Send')
      .container
      != messages('message', locals)
      if errors
        each error, i in errors
          div(class="alert alert-danger") #{error.msg}
      block content
      br
      hr
